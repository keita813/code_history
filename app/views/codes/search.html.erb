<div class="container">
	<div class="row">
		<div class="col-xs-3 col-xs-offset-1">
			<h2>検索一覧</h2>
		</div>
	</div>
	<div class="row">
		<div class="col-xs-10 col-xs-offset-1">
			<table class="table table-hover">
				<tbody>
					<% @codes.each do |code| %>
					<!-- 非同期のいいね -->
					<tr class="code_<%= code.id %>">
						<%= render 'code', code: code %>
					</tr>
					<% end %>
				</tbody>
			</table>
		</div>
	</div>
</div>